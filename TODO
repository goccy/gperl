# from perlfunc

# Functions for SCALARs or strings
* supported   :
* unsupported :
chomp, chop, chr, crypt, hex, index, lc, lcfirst, length,
oct, ord, pack, q/STRING/, qq/STRING/, reverse, rindex, sprintf, substr, tr///, uc, ucfirst, y///

# Regular expressions and pattern matching
* supported   :
* unsupported :
m//, pos, quotemeta, s///, split, study, qr//

# Numeric functions
* supported   :
* unsupported :
abs, atan2, cos, exp, hex, int, log, oct, rand, sin, sqrt, srand

# Functions for real @ARRAYs
* supported   :
* unsupported :
pop, push, shift, splice, unshift

# Functions for list data
* supported   :
* unsupported :
grep, join, map, qw/STRING/, reverse, sort, unpack

# Functions for real %HASHes
* supported   :
* unsupported :
delete, each, exists, keys, values

# Input and output functions
* supported   :
* unsupported :
binmode, close, closedir, dbmclose, dbmopen, die, eof,
fileno, flock, format, getc, print, printf, read, readdir,
rewinddir, seek, seekdir, select, syscall, sysread, sysseek,
syswrite, tell, telldir, truncate, warn, write

# Functions for fixed length data or records
* supported   :
* unsupported :
pack, read, syscall, sysread, syswrite, unpack, vec

# Functions for filehandles, files, or directories
* supported   :
* unsupported :
-X, chdir, chmod, chown, chroot, fcntl, glob, ioctl, link, lstat, mkdir, open, opendir, readlink, rename, rmdir, stat, symlink, umask, unlink, utime

# Keywords related to the control flow of your perl program
* supported   :
* unsupported :
caller, continue, die, do, dump, eval, exit, goto, last, next, redo, return, sub, wantarray

# Keywords related to scoping
* supported   :
* unsupported :
caller, import, local, my, our, package, use

# Miscellaneous functions
* supported   :
* unsupported :
defined, dump, eval, formline, local, my, our, reset, scalar, undef, wantarray

# Functions for processes and process groups
* supported   :
* unsupported :
alarm, exec, fork, getpgrp, getppid, getpriority, kill, pipe, qx/STRING/, setpgrp, setpriority, sleep, system, times, wait, waitpid

# Keywords related to perl modules
* supported   :
* unsupported :
do, import, no, package, require, use

# Keywords related to classes and object-orientedness
* supported   :
* unsupported :
bless, dbmclose, dbmopen, package, ref, tie, tied, untie, use

# Low-level socket functions
* supported   :
* unsupported :
accept, bind, connect, getpeername, getsockname, getsockopt,
listen, recv, send, setsockopt, shutdown, socket, socketpair

# System V interprocess communication functions
* supported   :
* unsupported :
msgctl, msgget, msgrcv, msgsnd, semctl, semget, semop, shmctl, shmget, shmread, shmwrite

# Fetching user and group info
* supported   :
* unsupported :
endgrent, endhostent, endnetent, endpwent, getgrent, getgrgid, getgrnam, getlogin, getpwent, getpwnam, getpwuid, setgrent, setpwent

# Fetching network info
* supported   :
* unsupported :
endprotoent, endservent, gethostbyaddr, gethostbyname, gethostent,
getnetbyaddr, getnetbyname, getnetent, getprotobyname, getprotobynumber,
getprotoent, getservbyname, getservbyport, getservent, sethostent, setnetent, setprotoent, setservent

# Time-related functions
* supported   :
* unsupported :
gmtime, localtime, time, times

# Functions new in perl5
* supported   :
* unsupported :
abs, bless, chomp, chr, exists, formline, glob, import, lc, lcfirst,
map, my, no, our, prototype, qx, qw, readline, readpipe, ref, sub*,
sysopen, tie, tied, uc, ucfirst, untie, use

# Functions obsoleted in perl5
* supported   :
* unsupported :
dbmclose, dbmopen
